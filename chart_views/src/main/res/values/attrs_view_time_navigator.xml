<resources>
    <declare-styleable name="TimeNavigatorView">
        <attr name="navigator_reference_date" format="string" />
        <attr name="navigator_range_unit" format="enum">
            <enum name="Year" value="1"/>
            <enum name="Month" value="2"/>
            <enum name="Day" value="5"/>
            <enum name="DayOfMonth" value="5"/>
            <enum name="Hour" value="11"/>
            <enum name="HourOfDay" value="11"/>
            <enum name="Minute" value="12"/>
            <enum name="Second" value="13"/>
        </attr>
        <attr name="navigator_range_qty" format="integer" />
        <attr name="navigator_range_offset" format="integer" />
        <attr name="navigator_datetime_format" format="string" />
        <attr name="navigator_text_appearance_btn" format="reference" />
        <attr name="navigator_text_appearance_txt" format="reference" />
    </declare-styleable>
</resources>